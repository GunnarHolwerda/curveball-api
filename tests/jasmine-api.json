{
    "spec_files": [
        "**/*.api.spec.ts"
    ],
    "stopSpecOnExpectationFailure": false,
    "random": false,
    "reporters": [
        {
            "name": "jasmine-spec-reporter#SpecReporter",
            "options": {
                "spec": {
                    "displayStacktrace": true,
                    "displayDuration": true
                },
                "summary": {
                    "displayPending": false
                }
            }
        }
    ]
}